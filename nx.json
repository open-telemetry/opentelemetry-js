{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "compile",
          "lint",
          "version"
        ]
      }
    }
  },
  "targetDefaults": {
    "compile": {
      "dependsOn": [
        "protos:generate",
        "version",
        "^compile"
      ],
      "inputs": [
        "{projectRoot}/src",
        "{projectRoot}/test"
      ],
      "outputs": [
        "{projectRoot}/build"
      ]
    },
    "test": {
      "dependsOn": [
        "compile"
      ]
    },
    "test:bench": {
      "dependsOn": [
        "compile"
      ]
    },
    "test:browser": {
      "dependsOn": [
        "compile"
      ]
    },
    "test:webworker": {
      "dependsOn": [
        "compile"
      ]
    },
    "lint": {
      "inputs": [
        "{projectRoot}/src",
        "{projectRoot}/test"
      ]
    },
    "version": {
      "inputs": [
        "{projectRoot}/package.json"
      ],
      "outputs": [
        "{projectRoot}/src/version.ts"
      ],
      "dependsOn": [
        "^version"
      ]
    },
    "watch": {
      "dependsOn": [
        "compile"
      ]
    }
  }
}