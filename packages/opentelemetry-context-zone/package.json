{
  "name": "@opentelemetry/context-zone",
  "version": "2.5.1",
  "description": "OpenTelemetry Context Zone",
  "author": "OpenTelemetry Authors",
  "homepage": "https://github.com/open-telemetry/opentelemetry-js/tree/main/packages/opentelemetry-context-zone",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "./build/index.cjs",
  "module": "./build/index.mjs",
  "types": "./build/index.d.cts",
  "exports": {
    ".": {
      "esnext": {
        "types": "./build/index.d.mts",
        "default": "./build/index.mjs"
      },
      "require": {
        "types": "./build/index.d.cts",
        "default": "./build/index.cjs"
      },
      "import": {
        "types": "./build/index.d.mts",
        "default": "./build/index.mjs"
      }
    }
  },
  "repository": "open-telemetry/opentelemetry-js",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "tsdown",
    "clean": "rimraf build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "version": "node ../../scripts/version-update.js",
    "watch": "tsdown --watch",
    "prewatch": "npm run precompile",
    "peer-api-check": "node ../../scripts/peer-api-check.js",
    "align-api-deps": "node ../../scripts/align-api-deps.js"
  },
  "keywords": [
    "opentelemetry",
    "web",
    "tracing",
    "profiling",
    "metrics",
    "stats"
  ],
  "engines": {
    "node": "^18.19.0 || >=20.6.0"
  },
  "files": [
    "build/**/*.cjs",
    "build/**/*.mjs",
    "build/**/*.d.cts",
    "build/**/*.d.mts",
    "build/**/*.map",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@opentelemetry/context-zone-peer-dep": "2.5.1",
    "zone.js": "^0.11.0 || ^0.12.0 || ^0.13.0 || ^0.14.0 || ^0.15.0 || ^0.16.0"
  },
  "sideEffects": true
}
