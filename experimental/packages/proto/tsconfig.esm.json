{
  "extends": "../../../tsconfig.base.esm.json",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "build/esm",
    "noImplicitOverride": false,
    "noUnusedLocals": false,
    "allowJs": true,
    "tsBuildInfoFile": "build/esm/tsconfig.esm.tsbuildinfo"
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.js"
  ]
}
