  */
export const ATTR_{{ attribute.name | screaming_snake_case }}_VALUE_{{ espec.id | screaming_snake_case }} = {{ c.print(espec.value) }};
{% endfor %}
{%- endif %}
{%- endif %}
{% endfor %}
