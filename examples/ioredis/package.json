    "@opentelemetry/exporter-jaeger": "^0.6.1",
    "@opentelemetry/node": "^0.6.1",
    "@opentelemetry/plugin-ioredis": "^0.6.1",
    "@opentelemetry/tracing": "^0.6.1",
    "ioredis": "^4.14.1"
  },
  "homepage": "https://github.com/open-telemetry/opentelemetry-js#readme"
