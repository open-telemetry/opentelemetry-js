{
  "name": "@opentelemetry/instrumentation-grpc",
  "version": "0.212.0",
  "description": "OpenTelemetry instrumentation for `@grpc/grpc-js` rpc client and server for gRPC framework",
  "author": "OpenTelemetry Authors",
  "homepage": "https://github.com/open-telemetry/opentelemetry-js/tree/main/experimental/packages/opentelemetry-instrumentation-grpc",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "./build/index.cjs",
  "module": "./build/index.mjs",
  "types": "./build/index.d.cts",
  "exports": {
    ".": {
      "esnext": {
        "types": "./build/index.d.mts",
        "default": "./build/index.mjs"
      },
      "require": {
        "types": "./build/index.d.cts",
        "default": "./build/index.cjs"
      },
      "import": {
        "types": "./build/index.d.mts",
        "default": "./build/index.mjs"
      }
    }
  },
  "repository": "open-telemetry/opentelemetry-js",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "precompile": "npm run protos:generate",
    "compile": "tsdown",
    "clean": "rimraf build",
    "test": "npm run protos:generate && nyc mocha test/**/*.test.ts",
    "tdd": "npm run test -- --watch-extensions ts --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "version": "node ../../../scripts/version-update.js",
    "watch": "tsdown --watch",
    "prewatch": "node ../../../scripts/version-update.js",
    "peer-api-check": "node ../../../scripts/peer-api-check.js",
    "protos:generate": "cd test/fixtures && buf generate",
    "align-api-deps": "node ../../../scripts/align-api-deps.js"
  },
  "keywords": [
    "opentelemetry",
    "grpc",
    "nodejs",
    "tracing",
    "profiling",
    "instrumentation"
  ],
  "engines": {
    "node": "^18.19.0 || >=20.6.0"
  },
  "files": [
    "build/**/*.cjs",
    "build/**/*.mjs",
    "build/**/*.d.cts",
    "build/**/*.d.mts",
    "build/**/*.map",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@bufbuild/buf": "1.53.0",
    "@grpc/grpc-js": "^1.14.3",
    "@grpc/proto-loader": "^0.7.10",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/context-async-hooks": "2.5.1",
    "@opentelemetry/core": "2.5.1",
    "@opentelemetry/sdk-trace-base": "2.5.1",
    "@opentelemetry/sdk-trace-node": "2.5.1",
    "@protobuf-ts/grpc-transport": "2.11.1",
    "@protobuf-ts/plugin": "2.11.1",
    "@protobuf-ts/runtime": "2.11.1",
    "@protobuf-ts/runtime-rpc": "2.11.1",
    "@types/mocha": "10.0.10",
    "@types/node": "18.19.130",
    "@types/sinon": "17.0.4",
    "@typescript/vfs": "1.6.4",
    "mocha": "11.7.5",
    "nyc": "17.1.0",
    "sinon": "18.0.1",
    "typescript": "5.0.4"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.3.0"
  },
  "dependencies": {
    "@opentelemetry/instrumentation": "0.212.0",
    "@opentelemetry/semantic-conventions": "^1.29.0"
  },
  "sideEffects": false
}
