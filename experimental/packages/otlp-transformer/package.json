{
  "name": "@opentelemetry/otlp-transformer",
  "version": "0.212.0",
  "description": "Transform OpenTelemetry SDK data into OTLP",
  "author": "OpenTelemetry Authors",
  "homepage": "https://github.com/open-telemetry/opentelemetry-js/tree/main/experimental/packages/otlp-transformer",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "./build/index.cjs",
  "module": "./build/index.mjs",
  "types": "./build/index.d.cts",
  "exports": {
    ".": {
      "esnext": {
        "types": "./build/index.d.mts",
        "default": "./build/index.mjs"
      },
      "require": {
        "types": "./build/index.d.cts",
        "default": "./build/index.cjs"
      },
      "import": {
        "types": "./build/index.d.mts",
        "default": "./build/index.mjs"
      }
    }
  },
  "private": false,
  "repository": "open-telemetry/opentelemetry-js",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "precompile": "npm run protos",
    "compile": "tsdown",
    "clean": "rimraf build",
    "protos": "npm run submodule && npm run protos:generate",
    "protos:generate:js": "pbjs -t static-module -p ./protos -w es6 --dependency protobufjs/minimal.js --null-defaults -o ./src/generated/root.js ./protos/opentelemetry/proto/common/v1/common.proto ./protos/opentelemetry/proto/resource/v1/resource.proto ./protos/opentelemetry/proto/trace/v1/trace.proto ./protos/opentelemetry/proto/collector/trace/v1/trace_service.proto ./protos/opentelemetry/proto/metrics/v1/metrics.proto ./protos/opentelemetry/proto/collector/metrics/v1/metrics_service.proto ./protos/opentelemetry/proto/logs/v1/logs.proto ./protos/opentelemetry/proto/collector/logs/v1/logs_service.proto && node scripts/fix-protobuf-import.mjs",
    "protos:generate:ts": "pbts -o ./src/generated/root.d.ts ./src/generated/root.js",
    "protos:generate": "npm run protos:generate:js && npm run protos:generate:ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "tdd": "npm run test -- --watch-extensions ts --watch",
    "submodule": "git submodule sync --recursive && git submodule update --init --recursive",
    "test": "nyc mocha 'test/**/*.test.ts'",
    "test:browser": "vitest run --config vitest.config.browser.mts",
    "test:bench": "node test/performance/benchmark/micro-benchmarks.js && node test/performance/benchmark/index.js | tee .benchmark-results.txt",
    "prewatch": "node ../../../scripts/version-update.js",
    "watch": "npm run protos && tsdown --watch",
    "peer-api-check": "node ../../../scripts/peer-api-check.js",
    "align-api-deps": "node ../../../scripts/align-api-deps.js"
  },
  "keywords": [
    "opentelemetry",
    "nodejs",
    "grpc",
    "protobuf",
    "otlp",
    "tracing",
    "metrics"
  ],
  "engines": {
    "node": "^18.19.0 || >=20.6.0"
  },
  "files": [
    "build/**/*.cjs",
    "build/**/*.mjs",
    "build/**/*.d.cts",
    "build/**/*.d.mts",
    "build/**/*.map"
  ],
  "peerDependencies": {
    "@opentelemetry/api": "^1.3.0"
  },
  "devDependencies": {
    "@opentelemetry/api": "1.9.0",
    "@types/mocha": "10.0.10",
    "mocha": "11.7.5",
    "nyc": "17.1.0",
    "protobufjs-cli": "2.0.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@opentelemetry/api-logs": "0.212.0",
    "@opentelemetry/core": "2.5.1",
    "@opentelemetry/resources": "2.5.1",
    "@opentelemetry/sdk-logs": "0.212.0",
    "@opentelemetry/sdk-metrics": "2.5.1",
    "@opentelemetry/sdk-trace-base": "2.5.1",
    "protobufjs": "^7.0.0"
  },
  "sideEffects": false
}
