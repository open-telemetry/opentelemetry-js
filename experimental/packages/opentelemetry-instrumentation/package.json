{
  "name": "@opentelemetry/instrumentation",
  "version": "0.212.0",
  "description": "Base class for node which OpenTelemetry instrumentation modules extend",
  "author": "OpenTelemetry Authors",
  "homepage": "https://github.com/open-telemetry/opentelemetry-js/tree/main/experimental/packages/opentelemetry-instrumentation",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "./build/index.cjs",
  "module": "./build/index.mjs",
  "browser": {
    "./build/platform/index.mjs": "./build/platform/browser/index.mjs"
  },
  "types": "./build/index.d.cts",
  "exports": {
    ".": {
      "esnext": {
        "types": "./build/index.d.mts",
        "default": "./build/index.mjs"
      },
      "require": {
        "types": "./build/index.d.cts",
        "default": "./build/index.cjs"
      },
      "import": {
        "types": "./build/index.d.mts",
        "default": "./build/index.mjs"
      }
    },
    "./hook.mjs": "./hook.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-telemetry/opentelemetry-js.git"
  },
  "files": [
    "hook.mjs",
    "build/**/*.cjs",
    "build/**/*.mjs",
    "build/**/*.d.cts",
    "build/**/*.d.mts",
    "build/**/*.map",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "tsdown",
    "clean": "rimraf build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "tdd": "npm run tdd:node",
    "tdd:node": "npm run test -- --watch-extensions ts --watch",
    "tdd:browser": "vitest --config vitest.config.browser.mts",
    "test:cjs": "nyc mocha 'test/**/*.test.ts' --exclude 'test/browser/**/*.ts'",
    "test:esm": "nyc node --experimental-loader=./hook.mjs ../../../node_modules/mocha/bin/mocha 'test/node/*.test.mjs'",
    "test": "npm run test:cjs && npm run test:esm",
    "test:browser": "vitest run --config vitest.config.browser.mts",
    "version": "node ../../../scripts/version-update.js",
    "watch": "tsdown --watch",
    "prewatch": "node ../../../scripts/version-update.js",
    "peer-api-check": "node ../../../scripts/peer-api-check.js",
    "align-api-deps": "node ../../../scripts/align-api-deps.js"
  },
  "keywords": [
    "opentelemetry",
    "nodejs",
    "browser",
    "tracing",
    "profiling",
    "metrics",
    "stats"
  ],
  "bugs": {
    "url": "https://github.com/open-telemetry/opentelemetry-js/issues"
  },
  "dependencies": {
    "@opentelemetry/api-logs": "0.212.0",
    "import-in-the-middle": "^2.0.6",
    "require-in-the-middle": "^8.0.0"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.3.0"
  },
  "devDependencies": {
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/sdk-metrics": "2.5.1",
    "@types/mocha": "10.0.10",
    "@types/node": "18.19.130",
    "@types/sinon": "17.0.4",
    "mocha": "11.7.5",
    "nyc": "17.1.0",
    "sinon": "18.0.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": "^18.19.0 || >=20.6.0"
  },
  "sideEffects": false
}
