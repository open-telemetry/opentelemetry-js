{
  "name": "@opentelemetry/otlp-exporter-base",
  "version": "0.212.0",
  "description": "OpenTelemetry OTLP Exporter base (for internal use only)",
  "author": "OpenTelemetry Authors",
  "homepage": "https://github.com/open-telemetry/opentelemetry-js/tree/main/experimental/packages/otlp-exporter-base",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "./build/index.cjs",
  "module": "./build/index.mjs",
  "types": "./build/index.d.cts",
  "exports": {
    ".": {
      "esnext": {
        "types": "./build/index.d.mts",
        "default": "./build/index.mjs"
      },
      "require": {
        "types": "./build/index.d.cts",
        "default": "./build/index.cjs"
      },
      "import": {
        "types": "./build/index.d.mts",
        "default": "./build/index.mjs"
      }
    },
    "./node-http": {
      "esnext": {
        "types": "./build/node-http/index.d.mts",
        "default": "./build/node-http/index.mjs"
      },
      "require": {
        "types": "./build/node-http/index.d.cts",
        "default": "./build/node-http/index.cjs"
      },
      "import": {
        "types": "./build/node-http/index.d.mts",
        "default": "./build/node-http/index.mjs"
      }
    },
    "./browser-http": {
      "esnext": {
        "types": "./build/browser-http/index.d.mts",
        "default": "./build/browser-http/index.mjs"
      },
      "require": {
        "types": "./build/browser-http/index.d.cts",
        "default": "./build/browser-http/index.cjs"
      },
      "import": {
        "types": "./build/browser-http/index.d.mts",
        "default": "./build/browser-http/index.mjs"
      }
    }
  },
  "repository": "open-telemetry/opentelemetry-js",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "tsdown",
    "clean": "rimraf build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "tdd": "npm run test -- --watch-extensions ts --watch",
    "tdd:browser": "vitest --config vitest.config.browser.mts",
    "test": "nyc mocha 'test/**/*.test.ts' --exclude 'test/browser/**/*.ts'",
    "test:browser": "vitest run --config vitest.config.browser.mts",
    "version": "node ../../../scripts/version-update.js",
    "watch": "tsdown --watch",
    "prewatch": "npm run precompile",
    "align-api-deps": "node ../../../scripts/align-api-deps.js",
    "maint:regenerate-test-certs": "cd test/certs && ./regenerate.sh"
  },
  "keywords": [
    "opentelemetry",
    "nodejs",
    "tracing",
    "profiling",
    "metrics",
    "stats",
    "monitoring"
  ],
  "engines": {
    "node": "^18.19.0 || >=20.6.0"
  },
  "files": [
    "build/**/*.cjs",
    "build/**/*.mjs",
    "build/**/*.d.cts",
    "build/**/*.d.mts",
    "build/**/*.map",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@opentelemetry/core": "2.5.1",
    "@opentelemetry/otlp-transformer": "0.212.0"
  },
  "devDependencies": {
    "@babel/core": "7.27.1",
    "@babel/preset-env": "7.27.2",
    "@opentelemetry/api": "1.9.0",
    "@types/mocha": "10.0.10",
    "@types/node": "18.19.130",
    "@types/sinon": "17.0.4",
    "mocha": "11.7.5",
    "nyc": "17.1.0",
    "sinon": "18.0.1",
    "typescript": "5.0.4",
    "webpack": "5.104.1",
    "webpack-cli": "6.0.1"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.3.0"
  },
  "sideEffects": false
}
